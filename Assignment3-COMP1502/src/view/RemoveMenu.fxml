<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mru.tsc.controller.RemoveManager">
   <center>
      <AnchorPane prefHeight="537.0" prefWidth="865.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="20.0" layoutY="76.0" prefHeight="35.0" prefWidth="160.0" text="Enter Toy Serial Number: ">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <HBox prefHeight="27.0" prefWidth="865.0" style="-fx-background-color: #D3D3D3;">
               <children>
                  <Button fx:id="homeButton" mnemonicParsing="false" onAction="#homeButtonListener" prefHeight="27.0" prefWidth="63.0" text="Home" />
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonListener" prefHeight="27.0" prefWidth="71.0" text="Add Toy" />
                  <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeButtonListener" prefHeight="27.0" prefWidth="84.0" text="Remove Toy" />
               </children>
            </HBox>
            <ListView fx:id="text" layoutX="277.0" layoutY="37.0" prefHeight="486.0" prefWidth="572.0" />
            <Label layoutX="20.0" layoutY="187.0" text="Serial Number: " />
            <TextField fx:id="serialNumberText" layoutX="100.0" layoutY="221.0" />
            <Button fx:id="removeToyButton" layoutX="87.0" layoutY="336.0" mnemonicParsing="false" onAction="#removeToyListener" prefHeight="37.0" prefWidth="109.0" text="Remove" />
            <Button fx:id="searchButton" layoutX="89.0" layoutY="398.0" mnemonicParsing="false" onAction="#searchButtonListener" prefHeight="34.0" prefWidth="105.0" text="Search" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
